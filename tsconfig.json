{
    "compilerOptions": {
        "target": "ES2024",
        "module": "commonjs",
        "lib": [
            "ES2024",
        ],
        "outDir": "dist",
        "rootDir": "./",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "moduleResolution": "node"
    },
    "exclude": [
        "node_modules"
    ]
}